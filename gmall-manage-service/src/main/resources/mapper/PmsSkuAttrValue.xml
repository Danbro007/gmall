<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.danbro.gmall.manage.service.mapper.PmsSkuAttrValueMapper">
    <resultMap id="pmsAttrIdList" type="com.danbro.gmall.api.dto.PmsSkuAttrValueDto" autoMapping="true">

    </resultMap>

    <select id="selectAttrIdBySkuId" resultMap="pmsAttrIdList">
    SELECT DISTINCT value_id from pms_sku_attr_value  where sku_id in (${skuIdSetStr})
    </select>
</mapper>